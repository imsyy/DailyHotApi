    "start": "tsx src/index.ts"
  },
  "type": "module",
  "dependencies": {
    "@hono/node-server": "^1.11.2",
    "axios": "^1.7.2",
    "cheerio": "1.0.0-rc.12",
    "dayjs": "^1.11.11",
    "dotenv": "^16.4.5",
    "feed": "^4.2.2",
    "hono": "^4.4.3",
    "md5": "^2.3.0",
    "node-cache": "^5.1.2",
    "puppeteer": "^22.10.0",
    "puppeteer-cluster": "^0.24.0",
    "rss-parser": "^3.13.0",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.1",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "eslint": "^8.57.0",
    "prettier": "^3.3.0",
    "tsx": "^3.14.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=20"
  }
