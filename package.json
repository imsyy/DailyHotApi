{
  "name": "dailyhot-api",
  "version": "2.0.0-rc.5",
  "description": "An Api on Today's Hot list",
  "keywords": [
    "API",
    "RSS"
  ],
  "homepage": "https://github.com/imsyy/DailyHotApi#readme",
  "bugs": {
    "url": "https://github.com/imsyy/DailyHotApi/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imsyy/DailyHotApi.git"
  },
  "license": "MIT",
  "author": "imsyy",
  "main": "dist/index.js",
  "types": "src/types.d.ts",
  "declaration": true,
  "declarationMap": true,
  "sourceMap": true,
  "files": [
    "LICENSE",
    "README.md",
    "dist/**/*",
    "src/types.d.ts",
    "!dist/logs/**/*"
  ],
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "dev": "tsx watch --no-cache src/index.ts",
